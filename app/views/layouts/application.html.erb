<!DOCTYPE html>
<html>
<head>
  <title>They Got Fit</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag    "session", media: "all", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag    "user", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <link href='https://fonts.googleapis.com/css?family=Hind+Vadodara:400,600,500' rel='stylesheet' type='text/css'>

  <%= csrf_meta_tags %>

</head>
<body>



<%= render "/shared/header" %>



<% flash.each do |message_type, message| %>

    <%= content_tag(:div, message, class: "alert alert-#{message_type}") %>
<% end %>


<%= yield %>

<%= render "/shared/footer" %>

</body>
</html>
